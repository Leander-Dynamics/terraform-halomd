    "@angular/core": "~13.1.1",
    "@angular/forms": "~13.1.1",
    "@angular/platform-browser": "~13.1.1",
    "@angular/platform-browser-dynamic": "~13.1.1",
    "@angular/platform-server": "~13.1.1",
    "@angular/router": "~13.1.1",
    "@azure/msal-angular": "^2.3.0",
    "@azure/msal-browser": "^2.24.0",
    "@kolkov/angular-editor": "^3.0.0-beta.0",
    "@ng-bootstrap/ng-bootstrap": "^12.1.1",
    "@ng-select/ng-select": "^8.3.0",
    "@popperjs/core": "^2.11.5",
    "angular-datatables": "^13.1.0",
    "bootstrap": "^5.3.2",
    "bootstrap-icons": "^1.11.1",
    "chart.js": "^3.8.0",
    "chartjs-plugin-datalabels": "^2.0.0",
    "datatables.net": "^1.13.6",
    "datatables.net-bs5": "^1.13.6",
    "datatables.net-plugins": "^1.13.6",
